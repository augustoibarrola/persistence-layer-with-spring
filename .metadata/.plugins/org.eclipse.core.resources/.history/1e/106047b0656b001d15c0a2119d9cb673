package com.geryon.exception;

public class GeryonBankException extends Exception {

	public GeryonBankException() {
		// TODO Auto-generated constructor stub
	}

	public GeryonBankException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public GeryonBankException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public GeryonBankException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public GeryonBankException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

}
